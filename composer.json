{
    "name": "javaabu/bandeyri-pay-laravel",
    "description": "Bandeyri pay gateway integration for Laravel",
    "homepage": "https://github.com/Javaabu/bandeyri-pay-laravel",
    "license": "MIT",
    "keywords": [
        "javaabu",
        "laravel",
        "bandeyri-pay"
    ],
    "authors": [
        {
            "name": "Hussain Afeef",
            "email": "ibnahnajjaar@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "guzzlehttp/promises": "^1.5",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
        "phpunit/phpunit": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\BandeyriPay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\BandeyriPay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\BandeyriPay\\BandeyriPayServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
