{
    "name": "javaabu/bandeyri-pay-laravel",
    "description": "Bandeyri pay gateway integration for Laravel",
    "homepage": "https://github.com/Javaabu/bandeyri-pay-laravel",
    "license": "MIT",
    "keywords": [
        "javaabu",
        "laravel",
        "bandeyri-pay"
    ],
    "authors": [
        {
            "name": "Hussain Afeef",
            "email": "ibnahnajjaar@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.5 || ^10.5"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\BandeyriPay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\BandeyriPay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\BandeyriPay\\BandeyriPayServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
